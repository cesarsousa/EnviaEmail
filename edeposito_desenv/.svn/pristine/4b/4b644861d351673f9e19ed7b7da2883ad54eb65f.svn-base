			case CCBB_DEP_PF_PAIS
				'BR':
					mask '99999\-9999999999;0; '
					mandatory true
				otherwise
					mask '999999999999999;0; '
					mandatory false					
			end case
		end ED_DEP_PF_CEP

		/* Depositante pessoa fisica: Telefone
		gui_element ED_DEP_PF_FONE
			type textbox
			mandatory true

			case CCBB_DEP_PF_PAIS
				'BR':
					mask '\+55\(99\)9999999999999;0; '
				otherwise
					mask '\+ccccccccccccccc;0; '
			end case
		end ED_DEP_PF_FONE

		/* Depositante pessoa fisica: Fax
		gui_element ED_DEP_PF_FAX
			type textbox

			case CCBB_DEP_PF_PAIS
				'BR':
					mask '\+55\(99\)9999999999999;0; '
				otherwise
					mask '\+ccccccccccccccc;0; '
			end case
		end ED_DEP_PF_FAX

		/* Depositante pessoa fisica: Email
		gui_element ED_DEP_PF_EMAIL
			type textbox
			mandatory true
		end ED_DEP_PF_EMAIL

		gui_element REG_DEP_PF_EMAIL
			type regnoedit
            mandatory true
            value ED_DEP_PF_EMAIL
            set_inputmode(REG_DEP_PF_EMAIL,'rnmEMAIL')
    	end REG_DEP_PF_EMAIL

		/* Depositante pessoa fisica: Depositante tambem eh inventor
	/*  gui_element CHB_DEP_PF_INVENTOR_DEPOSITANTE
	/*  	type checkbox
	/*  end CHB_DEP_PF_INVENTOR_DEPOSITANTE
    /*  
	/*  /* Depositante pessoa fisica: Inventor depositante requer a nao publicacao do seu nome
	/*  gui_element CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO
