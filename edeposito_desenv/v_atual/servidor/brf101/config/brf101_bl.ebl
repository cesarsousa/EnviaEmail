gui_logical_group PG_NOME
	type page

	gui_element CLCN_NOME
		type collectionbox

		structure
			collection CLCN_DEPOSITANTE
				element CLCN_ITEM_DEP_PF
					content depositante_pessoa_fisica_ordem
				end CLCN_ITEM_DEP_PF

				element CLCN_ITEM_DEP_PJ
					content depositante_pessoa_juridica_ordem
				end CLCN_ITEM_DEP_PJ
			end CLCN_DEPOSITANTE

			collection CLCN_PROCURADOR

				enable (not (existe_procuracao))

				element CLCN_ITEM_PROCURADOR_PF
					content procurador_pessoa_fisica_ordem
				end CLCN_ITEM_PROCURADOR_PF

				element CLCN_ITEM_PROCURADOR_PJ
					content procurador_pessoa_juridica_ordem
				end CLCN_ITEM_PROCURADOR_PJ
			end CLCN_PROCURADOR

			collection CLCN_INVENTOR
				element CLCN_ITEM_INVENTOR
					content inventor_ordem
				end CLCN_ITEM_INVENTOR
			end CLCN_INVENTOR
		end structure
	end CLCN_NOME
	
	subset EPO_CLCN_ITEM_DEP_PJ_enable
		set 'CLCN_ITEM_DEP_PJ'
		object 'CLCN_ITEM_DEP_PJ'
		/*condition ((get_exists(CLCN_ITEM_DEP_PJ)) and (not get_value(CHB_MPE_DEP_PJ)))
		condition ( get_exists (CLCN_ITEM_DEP_PJ))
	end EPO_CLCN_ITEM_DEP_PJ_enable
	
	subset EPO_CLCN_ITEM_DEP_PF_enable
		set 'CLCN_ITEM_DEP_PF'
		object 'CLCN_ITEM_DEP_PF'
		condition get_exists (CLCN_ITEM_DEP_PF) and ( get_count (EPO_CLCN_ITEM_DEP_PJ_enable) = 0 )		
	end EPO_CLCN_ITEM_DEP_PF_enable

	subset EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_enable
		set 'CHB_DEP_PF_INVENTOR_DEPOSITANTE'
		object 'CHB_DEP_PF_INVENTOR_DEPOSITANTE'
		condition (get_value(CHB_DEP_PF_INVENTOR_DEPOSITANTE))
	end EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_enable

	subset EPO_CHB_MPE_DEP_PJ_enable
		set 'CHB_MPE_DEP_PJ'
		object 'CHB_MPE_DEP_PJ'
		condition (get_value(CHB_MPE_DEP_PJ))
	end EPO_CHB_MPE_DEP_PJ_enable

	subset EPO_CHB_INVENTOR_ANONIMATO_enable
		set 'CHB_INVENTOR_ANONIMATO'
		object 'CHB_INVENTOR_ANONIMATO'
		condition (get_value(CHB_INVENTOR_ANONIMATO))
	end EPO_CHB_INVENTOR_ANONIMATO_enable

	subset EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO_enable
		set 'CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO'
		object 'CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO'
		condition (get_value(CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO))
	end EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO_enable
	
	subset DEP_PJ_PAGAMENTO_GRU_enable
		set 'CHB_DEP_PJ_PAGAMENTO_GRU'
		object 'CHB_DEP_PJ_PAGAMENTO_GRU'
		condition get_value (CHB_DEP_PJ_PAGAMENTO_GRU)
	end DEP_PJ_PAGAMENTO_GRU_enable
	
	subset DEP_PF_PAGAMENTO_GRU_enable
		set 'CHB_DEP_PF_PAGAMENTO_GRU'
		object 'CHB_DEP_PF_PAGAMENTO_GRU'
		condition get_value (CHB_DEP_PF_PAGAMENTO_GRU)
	end DEP_PJ_PAGAMENTO_GRU_enable
	
	non_gui_element existe_depositante_responsavel_pelo_pagamento_gru
		case (get_count (DEP_PF_PAGAMENTO_GRU_enable) = 0) and (get_count (DEP_PJ_PAGAMENTO_GRU_enable) = 0)
			true : value false
			false : value true			
		end case			
	end existe_depositante_responsavel_pelo_pagamento_gru

	/*subset EPO_CCBB_DEP_PJ_PAIS_enable
	/*	set 'CCBB_DEP_PJ_PAIS'
	/*	object 'CCBB_DEP_PJ_PAIS'
	/*	condition (get_value(CCBB_DEP_PJ_PAIS))
	/*end EPO_CCBB_DEP_PJ_PAIS_enable


	/* Implementado manualmente
	/* Depositante pessoa fisica
	gui_logical_group depositante_eh_pessoa_fisica
		/* Used to display first applicant name in File Manager summary view
		set_first_applicant(BRF101, primeiro_depositante)

		/* Informacao de campos

		/* Verificacao de ordem do nome e sobrenome do depositante pessoa fisica
		non_gui_element depositante_pessoa_fisica_ordem
			case ED_DEP_PF_SOBRENOME + ED_DEP_PF_NOME
				'':
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value depositante_pessoa_fisica_vazio_ordem + " (tambem inventor)"
						otherwise
							value depositante_pessoa_fisica_vazio_ordem
					end case
				otherwise
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value ED_DEP_PF_NOME + ' ' + ED_DEP_PF_SOBRENOME + " (tambem inventor)"
						otherwise
							value ED_DEP_PF_NOME + ' ' +  ED_DEP_PF_SOBRENOME
					end case
			end case
		end depositante_pessoa_fisica_ordem

		/* Verificacao do nome e sobrenome do depositante pessoa fisica
		non_gui_element depositante_pessoa_fisica_campo
			case ED_DEP_PF_NOME + ED_DEP_PF_SOBRENOME
				'':
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value depositante_pessoa_fisica_vazio + " (tambem inventor)"
						otherwise
							value depositante_pessoa_fisica_vazio
					end case
				otherwise
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value ED_DEP_PF_NOME + ' ' + ED_DEP_PF_SOBRENOME + " (tambem inventor)"
						otherwise
							value ED_DEP_PF_NOME + ' ' + ED_DEP_PF_SOBRENOME
					end case
			end case
		end depositante_pessoa_fisica_campo

		non_gui_element primeiro_depositante
			case get_exists(CLCN_DEPOSITANTE, 1)
				true:
					case get_name(CLCN_DEPOSITANTE, 1)
						'CLCN_ITEM_DEP_PJ':
							case get_ED_DEP_PJ_EMPRESA(CLCN_DEPOSITANTE, 1)
								'':
									value depositante_pessoa_juridica_vazio
								otherwise
									value get_ED_DEP_PJ_EMPRESA(CLCN_DEPOSITANTE, 1)
							end case
						'CLCN_ITEM_DEP_PF':
							case get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1) + get_ED_DEP_PF_NOME(CLCN_DEPOSITANTE, 1)
								'':
									case get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1)
										true:
											value depositante_pessoa_fisica_vazio + " (tambem inventor)"
										otherwise
											value depositante_pessoa_fisica_vazio
									end case
								otherwise
									case get_CHB_DEP_PF_INVENTOR_DEPOSITANTE(CLCN_DEPOSITANTE, 1)
										true:
											value get_ED_DEP_PF_NOME(CLCN_DEPOSITANTE, 1) + ' ' + get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1) + " (tambem inventor)"
										otherwise
											value get_ED_DEP_PF_NOME(CLCN_DEPOSITANTE, 1) + ' ' + get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1)
									end case
							end case
						otherwise
							value 'error'
					end case
				otherwise
					value ''
			end case
		end primeiro_depositante
		

		/* Campos de nome e endereco

		/* Depositante pessoa fisica: Sobrenome
		gui_element ED_DEP_PF_SOBRENOME
			type textbox

			mandatory true
		end ED_DEP_PF_SOBRENOME	

		/* Depositante pessoa fisica: Nome
		gui_element ED_DEP_PF_NOME
			type textbox

			mandatory true
		end ED_DEP_PF_NOME
		
		/* Depositante pessoa fisica: Responsabilidade pagamento GRU
		gui_element CHB_DEP_PF_PAGAMENTO_GRU
			type checkbox
			
			case existe_depositante_responsavel_pelo_pagamento_gru
				true :
					case CHB_DEP_PF_PAGAMENTO_GRU
						true : 
							value true
							enable true							
						otherwise 
							value false
							enable false							
					end case
				false :
					enable true
