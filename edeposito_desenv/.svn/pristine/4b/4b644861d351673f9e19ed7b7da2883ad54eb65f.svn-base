		object 'CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO'
		condition (get_value(CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO))
	end EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO_enable
	
	subset DEP_PJ_PAGAMENTO_GRU_enable
