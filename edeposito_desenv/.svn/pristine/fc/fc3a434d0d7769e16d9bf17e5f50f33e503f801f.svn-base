[General]

# Enrol WebSite
EnrolWebsite=https://edeposito.inpi.gov.br/credenciamento/

# GRU WebSite
GRUWebsite=http://formulario.inpi.gov.br/e-inpi/internetCliente/Principal.jsp

# log information level
DebugMode=1

# FM code
FormCode=FMMNGR

# FM build
FormBuild=FMMNGR1000

# Service name, as created during the server installation
ServiceName=EDEPOSITO_FMMNGR_Service

# service name for the list of services
ServiceDisplayName=E-DEPOSITO FM Server (FireBird)

# Country code for About
CountryCode=EP

# application name for Server Manager
Application name=EDITOR

# additional version info for About and XML
software-version additional=FM_FOP0205

# is software certificate used? (1-yes, 0-no)
SoftwareSecurity=0

# list of recognized PKCS11 libraries with parameters
PKCS11LibraryName=PkcsV2GK.dll,0,0;icpkcsi.dll;id2cbox.dll;settoki.dll;unipk11.dll;UsrPkcs11.dll,64,128,1000,0,1

# FM - default language
Default language=pt

# FM - list of supported languages
#Languages=pt,en,es
Languages=pt

# start internal web-server (1-no, 0-yes)
DisableWebServer=0

# disable CORBA (1=yes, 0=no)
DisableCORBA=0

# Server IP address

# web-help port number
WebPort=88

# CORBA protocol port number
CorbaPort=51000

# SOAP protocol port number
SOAPPort=61000

# FM time stamp of installation
Time stamp=40956,0703010301

# sending, proxy server name
ProxyServer=

# sending, proxy port number
ProxyPort=

# sending, proxy user name
ProxyUser=

# sending, proxy user password (hashed)
ProxyPassword=

# sending SSL version
SSL Version=SSLv3

# sending block size
BlockSize=1000000

# EFIP protocol parameter RecursiveCount
RecursiveCount=10

# EFIP protocol parameter RecursiveTimeOut
RecursiveTimeOut=5

# EFIP protocol parameter GetStatusTimeOut
GetStatusTimeOut=5

# database BLOB size for file storage (default 256K).
# DBChunkSize=

# DB2 ODBC driver
DB2_Driver=${DB2_DRIVER}

# DB2 server IP address
DB2_HostName=${DB2_HOSTNAME}

# DB2 communication port number
DB2_Port=${DB2_PORT}

# DB2 user name
DB2_UID=${DB2_UID}

# DB2 user password
DB2_PWD=${DB2_PWD}

# current working database schema
DB2_CurrentSchema=${DB2_CURRENT_SCHEMA}

# parameter ServerSend means:
# server or client sends filing (1-server, 0-client)
ServerSend=0

# Sending URL
SendingURL=${SENDING_URL}

# Limit of the number of applications that a user can create
MaxUserApplications=${_MAX_APPLICATIONS_PER_USER}
PMSSSLOnly=0
DataBase=FIREBIRD
Database Version=5.2
Firebird=251
Activated=1
Mandatory=1
IPAddress=localhost
#1=Mostra ao usuario tipo de assinatura
ShowWarningBeforeSend=1

[Location]
# path to main application folder
APPL_HOME=#INSTDIR#\fm

# exe path
ExeName=#INSTDIR#\fm\bin\eDEPfm.exe
ElfName=#INSTDIR#\fm\bin\eDEPfm.elf

# DNS or path to Production database
DataBaseProduction=#INSTDIR#\db\eDEP.gdb

# DNS or path to demo database
DataBaseDemo=#INSTDIR#\db\eDEP_d.gdb

# path to updates folder
UpdateDir=#INSTDIR#\fm\config\Update

SoftwareKeyFileName=#INSTDIR#\fm\config\client.p12

CADir=#INSTDIR#\fm\config\certs
FMConfigFile=#INSTDIR#\fm\config\eDEPfm.conf
ServerTools=#INSTDIR#\tools\smanager\bin\eDEPSManager.exe
