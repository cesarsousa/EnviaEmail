		/* Inventor: Inventor quer anonimato
		gui_element CHB_INVENTOR_ANONIMATO
			type checkbox

			case (get_count(EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO_enable) = 0)
				false:
					value false
					enable false
				otherwise
					case CHB_INVENTOR_ANONIMATO
						true:
							enable true
						otherwise
							case (get_count(EPO_CHB_INVENTOR_ANONIMATO_enable) = 0)
								true:
									enable true
								otherwise
									value false
									enable false
							end case
					end case
			end case

		end CHB_INVENTOR_ANONIMATO

