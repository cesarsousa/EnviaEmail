			case RB_CERTIFICADO_ADICAO
				false:
					value ''
			end case

			case CB_NUM_CERT_ADICAO
				'1':
					enable true
					mandatory true
					/*mask '999999999999'
					mask '99\ 9999\ 999999\ 9;0'
				'':
					enable false
					mandatory false
					mask ''
			otherwise
				enable true
				mandatory true
				/* mask '9999999'
				mask '9999999\-9;0'
			end case

		end ED_NUM_CERT_ADICAO

		gui_element REG_NUM_CERT_ADICAO
			type regnoedit

			/*case RB_CERTIFICADO_ADICAO
			/*	false:
			/*		value '0'
			/*end case

			case CB_NUM_CERT_ADICAO
				'1':
					/* Certificado de adição para BR
					enable true
					mandatory true
					set_inputmode(REG_NUM_CERT_ADICAO,'rnmBRINPIBR')

					/*case (length(ED_NUM_CERT_ADICAO)>=11)
					/*	true:
					/*		value ED_NUM_CERT_ADICAO
					/*	otherwise
					/*		value '0'
					/*end case
					value ED_NUM_CERT_ADICAO
				'':
					enable false
					mandatory false
			/*		mask ''
			otherwise
				/* Certificado de adição para PI e MU
				enable true
				mandatory true
				set_inputmode(REG_NUM_CERT_ADICAO,'rnmBRINPIPI')

				/*case (length(ED_NUM_CERT_ADICAO)>=6)
				/*	true:
				/*		value ED_NUM_CERT_ADICAO
				/*	otherwise
				/*		value '0'
				/*end case
				value ED_NUM_CERT_ADICAO
			end case

		end REG_NUM_CERT_ADICAO

	end TPN_PEDIDO

	gui_logical_group TPN_PEDIDO_DIVIDIDO
		gui_element CB_PEDIDO_DIVIDIDO
			type checkbox
			enable true			
		end CB_PEDIDO_DIVIDIDO

		gui_element TPN_NUM_PEDIDO_DIVIDIDO
			type panel

			enable CB_PEDIDO_DIVIDIDO
		end TPN_NUM_PEDIDO_DIVIDIDO

		gui_element CB_SIG_NUM_DIVIDIDO
			type itemlist

			case CB_PEDIDO_DIVIDIDO
				true:
					enable true
				otherwise
					value ''
					enable false
			end case

		end CB_SIG_NUM_DIVIDIDO

		gui_element ED_NUM_PEDIDO_DIVIDIDO
			type textbox

			case CB_PEDIDO_DIVIDIDO
				false:
					value ''
			end case

			case CB_SIG_NUM_DIVIDIDO
				'01':
					enable true
					mandatory true
					/* mask '999999999999'
					mask '99\ 9999\ 999999\ 9;0'
				'':
					enable false
					mandatory false
					mask ''
				otherwise
					enable true
					mandatory true
					mask '9999999\-9;0'
			end case
		end ED_NUM_PEDIDO_DIVIDIDO

		gui_element REG_NUM_PEDIDO_DIVIDIDO
			type regnoedit

			case CB_SIG_NUM_DIVIDIDO
				'01':
					/* Certificado de adição para BR
					enable true
					mandatory true
					set_inputmode(REG_NUM_PEDIDO_DIVIDIDO,'rnmBRINPIBR')

					/*case (length(ED_NUM_PEDIDO_DIVIDIDO)>=11)
					/*	true:
					/*		value ED_NUM_PEDIDO_DIVIDIDO
					/*	otherwise
					/*		value '0'
					/*end case
					value ED_NUM_PEDIDO_DIVIDIDO
				'':
					enable false
					mandatory false
			/*		mask ''
			otherwise
				/* Certificado de adição para PI, MU ou CA
				enable true
				mandatory true
				set_inputmode(REG_NUM_PEDIDO_DIVIDIDO,'rnmBRINPIPI')

				/*case (length(ED_NUM_PEDIDO_DIVIDIDO)>=6)
				/*	true:
				/*		value ED_NUM_PEDIDO_DIVIDIDO
				/*	otherwise
				/*		value '0'
				/*end case
				value ED_NUM_PEDIDO_DIVIDIDO
			end case

		end REG_NUM_PEDIDO_DIVIDIDO


		/* VERIFICAR
		non_gui_element pedido_campo
			case ED_NUM_PEDIDO_DIVIDIDO
				'':
					value pedido_campo_vazio
				otherwise
					value CB_SIG_NUM_DIVIDIDO + ED_NUM_PEDIDO_DIVIDIDO
			end case
		end pedido_campo
	end TPN_PEDIDO_DIVIDIDO

	/***********************************************************
	/* Validacoes da pagina Pedido
	/**********************************************************
	
	validation numero_pedido_dividido_validacao_nao_br
		id 1002
		level red
		definition (
						(CB_PEDIDO_DIVIDIDO) and
						(
							get_value(CB_SIG_NUM_DIVIDIDO) <> '01') and
							(not ED_NUM_PEDIDO_DIVIDIDO in mask '[0-9]{8}')
						)
		id_string numero_pedido_dividido_mensagem_validacao
	end numero_pedido_dividido_validacao_nao_br

	validation numero_pedido_dividido_validacao_br
		id 1003
		level red
		definition (
						(CB_PEDIDO_DIVIDIDO) and
						(get_value(CB_SIG_NUM_DIVIDIDO) = '01') and
						(
							(not ED_NUM_PEDIDO_DIVIDIDO in mask '1[0-3]{1}[0-9]{11}') and
							(not ED_NUM_PEDIDO_DIVIDIDO in mask '2[0-2]{1}[0-9]{11}')
						)
					)
		id_string numero_pedido_dividido_mensagem_validacao
	end numero_pedido_dividido_validacao_br

	validation numero_certificado_adicao_validacao_nao_br
		id 1004
		level red
		definition (
						(RB_CERTIFICADO_ADICAO) and
						(get_value(CB_NUM_CERT_ADICAO) <> '1') and
						(not ED_NUM_CERT_ADICAO in mask '[0-9]{8}')
					)
		id_string numero_certificado_adicao_mensagem_validacao
	end numero_certificado_adicao_validacao_nao_br

	validation numero_pedido_dividido_dv_validacao_PI_MU_CA
		id 1005
		level red
		definition (
						(CB_PEDIDO_DIVIDIDO) and
						(get_value(CB_SIG_NUM_DIVIDIDO)<>'01') and
						(not checkregnumberno(REG_NUM_PEDIDO_DIVIDIDO,'rnmBRINPIPI'))
					)
		id_string numero_pedido_dividido_dv_mensagem_validacao
	end numero_pedido_dividido_dv_validacao_PI_MU_CA

	validation numero_pedido_dividido_dv_validacao_BR
		id 1006
		level red
		definition (
						(CB_PEDIDO_DIVIDIDO) and
						(get_value(CB_SIG_NUM_DIVIDIDO)='01') and
						(not checkregnumberno(REG_NUM_PEDIDO_DIVIDIDO,'rnmBRINPIBR'))
					)
		id_string numero_pedido_dividido_dv_mensagem_validacao
	end numero_pedido_dividido_dv_validacao_BR

	validation numero_certificado_adicao_validacao_br
		id 1007
		level red
		definition (
						(RB_CERTIFICADO_ADICAO) and
						(
							(get_value(CB_NUM_CERT_ADICAO) = '1') and
							(
								(not ED_NUM_CERT_ADICAO in mask '[1]{1}[0-2]{1}[0-9]{11}')
							)
						)
				   )

		id_string numero_certificado_adicao_mensagem_validacao
	end numero_certificado_adicao_validacao_br

	validation numero_certificado_adicao_dv_validacao_PI_MU
		id 1008
		level red
		definition (
						(RB_CERTIFICADO_ADICAO) and
						(get_value(CB_NUM_CERT_ADICAO)<>'1') and
						(not checkregnumberno(REG_NUM_CERT_ADICAO,'rnmBRINPIPI'))
					)
		id_string numero_certificado_adicao_dv_mensagem_validacao
	end numero_certificado_adicao_dv_validacao_PI_MU

	validation numero_certificado_adicao_dv_validacao_BR
		id 1009
		level red
		definition (
						(RB_CERTIFICADO_ADICAO) and
						(get_value(CB_NUM_CERT_ADICAO)='1') and
						(not checkregnumberno(REG_NUM_CERT_ADICAO,'rnmBRINPIBR'))
					)
		id_string numero_certificado_adicao_dv_mensagem_validacao
	end numero_certificado_adicao_dv_validacao_BR

	validation arquivo_doc_periodo_de_graca_validacao
		id 1010
		level red
		definition CB_GRACA and (get_document_count(PNL_GRACA) = 0)
		id_string arquivo_doc_periodo_de_graca_mensagem_validacao
	end arquivo_doc_periodo_de_graca_validacao
	
	validation titulo_em_branco_validacao
		id 1001
		level red
		definition ED_TITULO_INVENCAO is empty
		id_string titulo_em_branco_mensagem_validacao
	end titulo_em_branco_validacao
	
	validation titulo_tamanho_validacao
		id 1011
		level red
		definition length ( ED_TITULO_INVENCAO ) > 9000
		id_string titulo_tamanho_mensagem_validacao
	end titulo_tamanho_validacao
	
end PG_PEDIDO

/*************************************************************************
