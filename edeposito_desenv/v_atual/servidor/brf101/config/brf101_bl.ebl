/***********************************************************
/* Pagina de Prioridade
/***********************************************************

gui_logical_group PG_PRIORIDADE
	type page
	
	non_gui_element existe_prioridade_anexada
		value get_exists (PNL_PRIORIDADE_INTERNA) or get_exists (PNL_PRIORIDADE_UNIONISTA) or get_exists (PNL_PRIORIDADE_UNIONISTA_TRADUC)
	end existe_prioridade_anexada

	gui_element CLCN_ITEM_PRIORIDADE
		type collectionbox	

		structure
			collection CLCN_ITEM_PRIORIDADE
				element PNL_PRIORIDADE_INTERNA						
				end PNL_PRIORIDADE_INTERNA
				
				element PNL_PRIORIDADE_UNIONISTA							
				end PNL_PRIORIDADE_UNIONISTA
				
				element PNL_PRIORIDADE_UNIONISTA_TRADUC								
				end PNL_PRIORIDADE_UNIONISTA_TRADUC				
				
			end CLCN_ITEM_PRIORIDADE
		end structure
	end CLCN_ITEM_PRIORIDADE
	
	gui_logical_group TPN_DECLARA_PRIORIDADE
		
		gui_element CB_DECLARA_PRIORIDADE
			type checkbox
			
			case existe_prioridade_anexada
				true:
					enable true
				false:
					enable false
					value false
			end case
		end CB_DECLARA_PRIORIDADE	
		
	end TPN_DECLARA_PRIORIDADE

	/* Prioridade Interna
	gui_logical_group prioridade_interna

		gui_element CB_PRIORIDADE_INTERNA_NUMERO
			type itemlist
			mandatory true
		end CB_PRIORIDADE_INTERNA_NUMERO		

		gui_element ED_PRIORIDADE_INTERNA_NUMERO
			type textbox		

			case CB_PRIORIDADE_INTERNA_NUMERO
				'01':
					enable true
					mandatory true
					mask '99\ 9999\ 999999\ 9;0; '
				'':
					enable false
					mandatory false
					mask ''
				otherwise
					enable true
					mandatory true
					mask '9999999\-99999999;0; '
			end case
		end ED_PRIORIDADE_INTERNA_NUMERO

		gui_element REG_PRIORIDADE_INTERNA_NUMERO
			type regnoedit

			case CB_PRIORIDADE_INTERNA_NUMERO
				'01':					
					enable true
					mandatory true
					set_inputmode(REG_PRIORIDADE_INTERNA_NUMERO,'rnmBRINPIBR')					
					value ED_PRIORIDADE_INTERNA_NUMERO
				'':
					enable false
					mandatory false			
			otherwise
				enable true
				mandatory true
				set_inputmode(REG_PRIORIDADE_INTERNA_NUMERO,'rnmBRINPIPI')				
				value ED_PRIORIDADE_INTERNA_NUMERO
			end case

		end REG_PRIORIDADE_INTERNA_NUMERO	
		
		gui_element ED_PRIORIDADE_INTERNA_ESCRITORIO
			type textbox
			default 'Instituto Nacional da Propriedade Industrial (INPI)'
			enable false
			mandatory true
		end ED_PRIORIDADE_INTERNA_ESCRITORIO
	
		gui_element EDD_PRIORIDADE_INTERNA_DATA
			type datebox
			mandatory true
		end EDD_PRIORIDADE_INTERNA_DATA	
	
	end prioridade_interna
	
	/* Prioridade Unionista
	gui_logical_group prioridade_unionista	
		
		gui_element CCBB_PRIORIDADE_UNIONISTA_ESCRITORIO
			type itemlist			
			mandatory true
		end CCBB_PRIORIDADE_UNIONISTA_ESCRITORIO	
	
		gui_element ED_PRIORIDADE_UNIONISTA_NUMERO
			type textbox
			mandatory true
			visible CCBB_PRIORIDADE_UNIONISTA_ESCRITORIO <> 'BR'
			
			case CCBB_PRIORIDADE_UNIONISTA_ESCRITORIO = 'BR'
				true: value ''
			end case
			
		end ED_PRIORIDADE_UNIONISTA_NUMERO
		
		gui_element TPN_PRIORIDADE_UNIONISTA_BR_NUMERO
			type panel
			enable CCBB_PRIORIDADE_UNIONISTA_ESCRITORIO = 'BR'
		end TPN_PRIORIDADE_UNIONISTA_BR_NUMERO
		
		gui_element CB_PRIORIDADE_UNIONISTA_BR_NUMERO
			type itemlist
			mandatory true
		end CB_PRIORIDADE_UNIONISTA_BR_NUMERO		

		gui_element ED_PRIORIDADE_UNIONISTA_BR_NUMERO
			type textbox		

			case CB_PRIORIDADE_UNIONISTA_BR_NUMERO
				'01':
					enable true
					mandatory true
					mask '99\ 9999\ 999999\ 9;0; '
