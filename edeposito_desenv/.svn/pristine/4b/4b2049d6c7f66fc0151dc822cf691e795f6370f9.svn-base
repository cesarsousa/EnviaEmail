[General]

# log information level
DebugMode=2

#form code
FormCode=BRF113

#form code + build number
FormBuild=BRF113#COMPVER#

#name of the service created at server installation
ServiceName=EDEPOSITO_BRF113_Service

#service name shown in the server list
ServiceDisplayName=E-DEPOSITO BRF113 Server (FireBird)

#country code for About
CountryCode=BR

#patent office
LocalizeTitle=BR

#patent office ID
Localize=50

#form IÂ inside patent office
FormType=13

#form name (in FM tab Forms, Name)
TemplateName=DIRPA-FQ013 Solicitação para Programa de Patentes Verdes

#form type (tab Forms, Name in FM)
TemplateType=DIRPA-FQ013

#form description (tab Forms, Description in FM)
TemplateDescription=Solicitacao para Programa de Patentes Verdes

#plug-in name for displaying in Server Manager
Application name=DIRPA-FQ013

#additional version for displaying in About and in XML
software-version=FMMNGR1000, BRF113#COMPVER#, FM_FOP0205

#Fees code for national office
FeesCode=BR

#maximum Reference length for application
#default value is 15
#ReferenceLength=15

#defines if to show the pictures with validation level at plug-in panels (in NavigationBar)
#ShowPageValidation=1 is default value

#defines where form will be sent by default (Online or Phisical Media)
#default value is 0
Default Sender=3

#defines possibility of Send for Online and Phisical Media
# (MaskSender and 8) <> 0 - Online
# (iMaskSender and 16) <> 0  - Phisical Media
#Mask Sender=24 is default value

#input file for transformation from AnnexF into business-objects tree
#used during form import and opening
AnnexFFileName=brf113-request.xml

#file that opens first at form preview
FirstPreview=brf113-request.pdf

#if RO_URLs = 1,then URL for Send is read from the base depending on RO (patent office), which is defined in AnnexFFileName
#default is 0 (false)
RO_URLs=0

# disable CORBA (1=yes, 0=no)
DisableCORBA=0

# Server IP address
IPAddress=localhost

# web-help port number
WebPort=88

# CORBA protocol port number
CorbaPort=52013

# SOAP protocol port number
SOAPPort=62013

#installation time
Time stamp=40938.5066127662

#CheckOriginalFileName=1 - default 1(true)
#if true, then when file is being attached the check runs if there is another attached file with the same name

#if Logoname defined, then image LogoName is searched + [application launch language] and is output into splash form
#Logoname=

# Languages is a list of languages in which form can be launched
#Languages=pt,en,es
Languages=pt

# database BLOB size for file storage (default 256K).
# DBChunkSize=

# DB2 ODBC driver
DB2_Driver=${DB2_DRIVER}

# DB2 server IP address
DB2_HostName=${DB2_HOSTNAME}

# DB2 communication port number
DB2_Port=${DB2_PORT}

# DB2 user name
DB2_UID=${DB2_UID}

# DB2 user password
DB2_PWD=${DB2_PWD}

# current working database schema
DB2_CurrentSchema=${DB2_CURRENT_SCHEMA}

# CommandGUIEngine retry count. Default value = 120
CommandGUIEngineCount=120

# CommandGUIEngine wait timeout, ms. Default value = 1000ms (120 * 1000ms = 2 min waiting)
CommandGUIEngineTimeout=1000

# Old server initialization process (default = 0, i.e. new algorithm)
OldServerInitProcess=1
ProdEnabled=1
Activated=1
CDREnabled=0
CDRDir=c:\

[Location]
# path to main application folder
APPL_HOME=#INSTDIR#\brf113

# path to java directory
JAVA_HOME=#INSTDIR#\JRE_1_6_0_22

# parameters of FOP-transfornmation xml into pdf
FOPParam=-Djava.endorsed.dirs="#INSTDIR#\FOP\lib\endorsed" -Xmx256M -cp "#INSTDIR#\FOP\app\fop.jar;#INSTDIR#\FOP\lib\avalon-framework-cvs-20020315.jar;#INSTDIR#\FOP\lib\batik-1.5.jar;#INSTDIR#\FOP\lib\batik-awt-util-1.5.jar;#INSTDIR#\FOP\lib\batik-bridge-1.5.jar;#INSTDIR#\FOP\lib\batik-ext-1.5.jar;#INSTDIR#\FOP\lib\batik-gvt-1.5.jar;#INSTDIR#\FOP\lib\batik-svg-dom-1.5.jar;#INSTDIR#\FOP\lib\batik-svggen-1.5.jar;#INSTDIR#\FOP\lib\batik-util-1.5.jar;#INSTDIR#\FOP\lib\batik-css-1.5.jar;#INSTDIR#\FOP\lib\batik-dom-1.5.jar;#INSTDIR#\FOP\lib\batik-extension-1.5.jar;#INSTDIR#\FOP\lib\batik-gui-util-1.5.jar;#INSTDIR#\FOP\lib\batik-parser-1.5.jar;#INSTDIR#\FOP\lib\batik-script-1.5.jar;#INSTDIR#\FOP\lib\batik-transcoder-1.5.jar;#INSTDIR#\FOP\lib\batik-xml-1.5.jar;#INSTDIR#\FOP\lib\itext-2.1.7.jar;#INSTDIR#\FOP\lib\jai_codec-1.1.3.jar;#INSTDIR#\FOP\lib\jai_core-1.1.3.jar;#INSTDIR#\FOP\lib\jeuclid-2.0.jar" org.apache.fop.apps.Fop -c "#INSTDIR#\FOP\config\BRconfig.xml"
FOPParamLinux=-Djava.endorsed.dirs="#INSTDIR#\FOP/lib/endorsed" -Xmx256M -cp "#INSTDIR#\FOP\app\fop.jar;#INSTDIR#\FOP\lib\avalon-framework-cvs-20020315.jar;#INSTDIR#\FOP\lib\batik-1.5.jar;#INSTDIR#\FOP\lib\batik-awt-util-1.5.jar;#INSTDIR#\FOP\lib\batik-bridge-1.5.jar;#INSTDIR#\FOP\lib\batik-ext-1.5.jar;#INSTDIR#\FOP\lib\batik-gvt-1.5.jar;#INSTDIR#\FOP\lib\batik-svg-dom-1.5.jar;#INSTDIR#\FOP\lib\batik-svggen-1.5.jar;#INSTDIR#\FOP\lib\batik-util-1.5.jar;#INSTDIR#\FOP\lib\itext-2.1.7.jar;#INSTDIR#\FOP\lib\jai_codec-1.1.3.jar;#INSTDIR#\FOP\lib\jai_core-1.1.3.jar;#INSTDIR#\FOP\lib\jeuclid-2.0.jar;#INSTDIR#\FOP\lib\endorsed\xalan-2.7.0.jar;#INSTDIR#\FOP\lib\endorsed\xercesImpl-2.7.1.jar;#INSTDIR#\FOP\lib\endorsed\xml-apis-1.3.02.jar;#INSTDIR#\FOP\lib\itext-2.1.7.jar;#INSTDIR#\FOP\lib\jai_codec-1.1.3.jar;#INSTDIR#\FOP\lib\jai_core-1.1.3.jar;#INSTDIR#\FOP\lib\jeuclid-2.0.jar" org.apache.fop.apps.Fop -c "#INSTDIR#\FOP/config/BRconfig.xml"

# xalan parameters
XalanParam=-Xms64M -Xmx256M -cp "#INSTDIR#\FOP\lib\endorsed\xml-apis-1.3.02.jar;#INSTDIR#\FOP\lib\endorsed\xercesImpl-2.7.1.jar;#INSTDIR#\FOP\lib\endorsed\xalan-2.7.0.jar" org.apache.xalan.xslt.Process
XalanParamLinux=-Xms64M -Xmx256M -cp "#INSTDIR#\FOP/lib/endorsed/xml-apis-1.3.02.jar;#INSTDIR#\FOP/lib/endorsed/xercesImpl-2.7.1.jar;#INSTDIR#\FOP/lib/endorsed/xalan-2.7.0.jar" org.apache.xalan.xslt.Process

# exe path
ExeName=#INSTDIR#\brf113\bin\brf113.exe
ElfName=#INSTDIR#\brf113\bin\brf113.elf

# DNS or path to Production database
DataBaseProduction=#INSTDIR#\db\eDEP.gdb

# DNS or path to demo database
DataBaseDemo=#INSTDIR#\db\eDEP_d.gdb

# additional lgf for address book
#AddressbookLgf=

# Addressbook is a path to xml with description of additional controls for national offices in address book
Addressbook=#INSTDIR#\fm\config\addressbook\BRaddressbook.xml
FMConfigFile=#INSTDIR#\fm\config\eDEPfm.conf


[DTDs]
# path to directory with general dtd
Common=#INSTDIR#\fop\config\dtd

# paths to directories with dtd forms
BRF113=#INSTDIR#\brf113\config\dtd

