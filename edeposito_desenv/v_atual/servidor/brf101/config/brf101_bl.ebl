					value biologico_campo_vazio
				otherwise
					value ED_MATERIAL_BIO_IDENTIFICACAO
			end case
		end biologico_campo	

		gui_element CCBB_MATERIAL_BIO_INSTITUICAO
			type itemlist
			mandatory true
