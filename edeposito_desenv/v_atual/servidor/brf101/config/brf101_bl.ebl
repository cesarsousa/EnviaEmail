gui_logical_group PG_PEDIDO
	type page	
	
	/* GUI Logical Group: Natureza do pedido
	gui_logical_group TPN_PEDIDO
		/*GUI element: Titulo da invencao
		gui_element ED_TITULO_INVENCAO
			type textbox
			mandatory true
		end ED_TITULO_INVENCAO

		/* GUI Logical Group: Escolha da natureza
		gui_logical_group TPN_NATUREZA
			type radiogroup
				RB_PATENTE_INVENCAO
				RB_MODELO_UTILIDADE
				RB_CERTIFICADO_ADICAO
			end radiogroup
			default RB_PATENTE_INVENCAO
		end TPN_NATUREZA	

		gui_element TPN_PEDIDO_DIVIDIDO
			type panel

			case RB_CERTIFICADO_ADICAO
				true:
					enable false
				false:
					enable true
			 end case
		end TPN_PEDIDO_DIVIDIDO	

		gui_element RB_CERTIFICADO_ADICAO
			type radiobutton
			enable not CB_PEDIDO_DIVIDIDO
		end RB_CERTIFICADO_ADICAO

		gui_element CB_NUM_CERT_ADICAO
			type itemlist

			case RB_CERTIFICADO_ADICAO
				true:
					enable true
				otherwise
					value ''
					enable false
			end case
		end CB_NUM_CERT_ADICAO

		gui_element LBL_NUM_CERT_ADICAO
			type text
			enable RB_CERTIFICADO_ADICAO

		end LBL_NUM_CERT_ADICAO

		gui_element ED_NUM_CERT_ADICAO
