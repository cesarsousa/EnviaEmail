			/*		mandatory false
			/*		enable false
			/*end case


		end CCBB_INVENTOR_ESTADO	

		/* Regiao/Provincia/Estado Edit
		gui_element ED_INVENTOR_ESTADO
			type textbox

			mandatory false
			case CHB_INVENTOR_ANONIMATO
				false:
					case (CCBB_INVENTOR_PAIS = 'BR')
						true:
							enable false
							visible false
							value ''
						false:
							enable true
							visible true
					end case

				otherwise
					enable false
					value ''
			end case

		end ED_INVENTOR_ESTADO

		non_gui_element existe_inventor_sigilo
			case (get_count(EPO_CHB_INVENTOR_ANONIMATO_enable) = 0)
				true:
					value false
				false:
					value true
			end case
		end existe_inventor_sigilo

	end eh_inventor

