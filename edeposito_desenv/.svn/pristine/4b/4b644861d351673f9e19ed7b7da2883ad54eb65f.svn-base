					visible false
					value ''
				false:
					enable true
					visible true
			end case

		end ED_DEP_PF_ESTADO	

		non_gui_element existe_depositante_inventor_sigilo
			case (get_count(EPO_CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO_enable) = 0)
				true:
