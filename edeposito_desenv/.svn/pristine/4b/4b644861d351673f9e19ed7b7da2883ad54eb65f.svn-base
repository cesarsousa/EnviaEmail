	/* Implementado manualmente
	/* Depositante pessoa fisica
	gui_logical_group depositante_eh_pessoa_fisica
		/* Used to display first applicant name in File Manager summary view
		set_first_applicant(BRF104, primeiro_depositante)

		/* Informacao de campos

		/* Verificacao de ordem do nome e sobrenome do depositante pessoa fisica
		non_gui_element depositante_pessoa_fisica_ordem
			case ED_DEP_PF_SOBRENOME + ED_DEP_PF_NOME
				'':
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value depositante_pessoa_fisica_vazio + " (tambem inventor)"
						otherwise
							value depositante_pessoa_fisica_vazio
					end case
				otherwise
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value ED_DEP_PF_NOME + ' ' + ED_DEP_PF_SOBRENOME + " (tambem inventor)"
						otherwise
							value ED_DEP_PF_NOME + ' ' +  ED_DEP_PF_SOBRENOME
					end case
			end case
		end depositante_pessoa_fisica_ordem

		/* Verificacao do nome e sobrenome do depositante pessoa fisica
		non_gui_element depositante_pessoa_fisica_campo
			case ED_DEP_PF_NOME + ED_DEP_PF_SOBRENOME
				'':
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value depositante_pessoa_fisica_vazio + " (tambem inventor)"
						otherwise
							value depositante_pessoa_fisica_vazio
					end case
				otherwise
					case CHB_DEP_PF_INVENTOR_DEPOSITANTE
						true:
							value ED_DEP_PF_NOME + ' ' + ED_DEP_PF_SOBRENOME + " (tambem inventor)"
						otherwise
							value ED_DEP_PF_NOME + ' ' + ED_DEP_PF_SOBRENOME
					end case
			end case
		end depositante_pessoa_fisica_campo

		non_gui_element primeiro_depositante
			case get_exists(CLCN_DEPOSITANTE, 1)
				true:
					case get_name(CLCN_DEPOSITANTE, 1)
						'CLCN_ITEM_DEP_PJ':
							case get_ED_DEP_PJ_EMPRESA(CLCN_DEPOSITANTE, 1)
								'':
									value depositante_pessoa_juridica_vazio
								otherwise
									value get_ED_DEP_PJ_EMPRESA(CLCN_DEPOSITANTE, 1)
							end case
						'CLCN_ITEM_DEP_PF':
							case get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1) + get_ED_DEP_PF_NOME(CLCN_DEPOSITANTE, 1)
								'':
									case get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1)
										true:
											value depositante_pessoa_fisica_vazio + " (tambem inventor)"
										otherwise
											value depositante_pessoa_fisica_vazio
									end case
								otherwise
									case get_CHB_DEP_PF_INVENTOR_DEPOSITANTE(CLCN_DEPOSITANTE, 1)
										true:
											value get_ED_DEP_PF_NOME(CLCN_DEPOSITANTE, 1) + ' ' + get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1) + " (tambem inventor)"
										otherwise
											value get_ED_DEP_PF_NOME(CLCN_DEPOSITANTE, 1) + ' ' + get_ED_DEP_PF_SOBRENOME(CLCN_DEPOSITANTE, 1)
									end case
							end case
						otherwise
							value 'error'
					end case
				otherwise
					value ''
			end case
		end primeiro_depositante
		

		/* Campos de nome e endereco

		/* Depositante pessoa fisica: Sobrenome
		gui_element ED_DEP_PF_SOBRENOME
			type textbox

			mandatory true
		end ED_DEP_PF_SOBRENOME	

		/* Depositante pessoa fisica: Nome
		gui_element ED_DEP_PF_NOME
			type textbox

			mandatory true
		end ED_DEP_PF_NOME
		
		/* Depositante pessoa fisica: Responsabilidade pagamento GRU
		gui_element CHB_DEP_PF_PAGAMENTO_GRU
			type checkbox
			
			case existe_depositante_responsavel_pelo_pagamento_gru
				true :
					case CHB_DEP_PF_PAGAMENTO_GRU
						true : 
							value true
							enable true							
						otherwise 
							value false
							enable false							
					end case
				false :
					enable true
			end case	

		end ED_DEP_PF_NOME
		
		/* Depositante pessoa fisica: Titulo
