	/*  end CHB_DEP_PF_INVENTOR_DEPOSITANTE_ANONIMO

		/* Depositante pessoa fisica: Pais
		gui_element CCBB_DEP_PF_PAIS
			type itemlist

			mandatory true
		end CCBB_DEP_PF_PAIS

		/* Depositante pessoa fisica: Regiao/Provincia/Estado
		gui_element CCBB_DEP_PF_ESTADO
		   type itemlist

			case CCBB_DEP_PF_PAIS				
				'BR':
					table 'brBraSt' field 'Name'
				otherwise
					table '' field ''
			end case

			case CCBB_DEP_PF_PAIS				
				'BR':
					enable true
					mandatory true
				otherwise
					enable false
					value ''
					mandatory false
			end case
		end CCBB_DEP_PF_ESTADO

		/* Regiao/Provincia/Estado Edit
		gui_element ED_DEP_PF_ESTADO
			type textbox

			mandatory false

			case (CCBB_DEP_PF_PAIS = 'BR')
