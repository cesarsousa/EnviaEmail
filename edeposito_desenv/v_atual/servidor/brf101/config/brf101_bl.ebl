		gui_element ED_MATERIAL_BIO_NOME_INSTITUICAO
			type textbox
			case CCBB_MATERIAL_BIO_INSTITUICAO
				outro_codigo:
					mandatory true
				otherwise
					mandatory false
			end case

			case CCBB_MATERIAL_BIO_INSTITUICAO
				outro_codigo:
					table '' field ''
				otherwise
					table 'fmDepI' field 'Name'
			end case

			case CCBB_MATERIAL_BIO_INSTITUICAO
				outro_codigo:
					readonly false

					case is_in_dbcursor(ED_MATERIAL_BIO_NOME_INSTITUICAO, 'fmDepI', 'Name')
						true: default ''
					end case
				otherwise
					readonly true
					default CCBB_MATERIAL_BIO_INSTITUICAO
			end case		
			
		end ED_MATERIAL_BIO_NOME_INSTITUICAO		

		gui_element ME_MATERIAL_BIOLOGICO_INSTITUICAO_ENDERECO
			type textbox
			/* dbCursor plDepInst
			
			case CCBB_MATERIAL_BIO_INSTITUICAO
				outro_codigo:
					mandatory true
				otherwise
					mandatory false
			end case

			case CCBB_MATERIAL_BIO_INSTITUICAO
				outro_codigo:
					table '' field ''
				otherwise
					table 'fmDepI' field 'Adress'
			end case

			case CCBB_MATERIAL_BIO_INSTITUICAO
				outro_codigo:
					readonly false

					case is_in_dbcursor(ME_MATERIAL_BIOLOGICO_INSTITUICAO_ENDERECO, 'fmDepI', 'Adress')
						true: default ''
					end case
				otherwise
					readonly true
					default CCBB_MATERIAL_BIO_INSTITUICAO
			end case
		end ME_MATERIAL_BIOLOGICO_INSTITUICAO_ENDERECO
	end eh_biologico

