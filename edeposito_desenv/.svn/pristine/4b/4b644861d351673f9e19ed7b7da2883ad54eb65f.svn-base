/***********************************************************
/* Pagina do Pedido de Patente
/***********************************************************
gui_logical_group PG_PEDIDO
	type page
	
	gui_logical_group TPN_PEDIDO
		
		gui_element ED_TITULO_INVENCAO
			type textbox
			mandatory true
		end ED_TITULO_INVENCAO
		
		gui_element CB_NUM_CERT_ADICAO
			type itemlist
			mandatory true		
		end CB_NUM_CERT_ADICAO
		
		gui_element ED_NUM_CERT_ADICAO
			type textbox		

			case CB_NUM_CERT_ADICAO
				'01':
					enable true
					mandatory true
					mask '99\ 9999\ 999999\ 9;0'
				'':
					enable false
					mandatory false
					value ''
					mask ''
			otherwise
				enable true
				mandatory true
				mask '9999999\-9;0'
			end case

		end ED_NUM_CERT_ADICAO

		gui_element REG_NUM_CERT_ADICAO
			type regnoedit			

			case CB_NUM_CERT_ADICAO
				'01':
					/* Certificado de adição para BR
					enable true
					mandatory true
					set_inputmode (REG_NUM_CERT_ADICAO,'rnmBRINPIBR')				
					value ED_NUM_CERT_ADICAO
				'':
					enable false
					mandatory false			
			otherwise
				/* Certificado de adição para PI, MU e C1...9
				enable true
				mandatory true
				set_inputmode (REG_NUM_CERT_ADICAO,'rnmBRINPIPI')				
				value ED_NUM_CERT_ADICAO
			end case

		end REG_NUM_CERT_ADICAO

		non_gui_element iniciais_numero_de_patente_invencao
			value (('10' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 )) or ('11' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 )) or ('12' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 )))
		end iniciais_numero_de_pedido_patente
		
		non_gui_element iniciais_numero_de_modelo_utilidade
			value (('20' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 )) or ('21' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 )) or ('22' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 )))
		end iniciais_numero_de_modelo_utilidade
		
		non_gui_element iniciais_numero_de_certificado_adicao
			value ('13' = substr ( ED_NUM_CERT_ADICAO, 1 , 2 ))
		end iniciais_numero_de_certificado_adicao

		non_gui_element obter_tipo_alteracao
			case RB_REQUER_2
				true : value str_alteracao_nome
				false : 
					case RB_REQUER_3
						true : value str_alteracao_sede
						false : value str_documento_alteracao_nome_sede
					end case
			end case
		end obter_tipo_alteracao

		/* GUI Logical Group: Escolha da natureza
		gui_logical_group TPN_NATUREZA
			type radiogroup
				RB_PATENTE_INVENCAO
				RB_MODELO_UTILIDADE
				RB_CERTIFICADO_ADICAO
			end radiogroup
			
		    case CB_NUM_CERT_ADICAO
		    	'':
		    		set_value ( RB_PATENTE_INVENCAO, false )
		    		set_value ( RB_MODELO_UTILIDADE, false )
		    		set_value ( RB_CERTIFICADO_ADICAO, false )
		    		set_enable ( RB_PATENTE_INVENCAO, false )
		    		set_enable ( RB_MODELO_UTILIDADE, false )
		    		set_enable ( RB_CERTIFICADO_ADICAO, false )
		    	'01':
		    		set_value ( RB_PATENTE_INVENCAO, iniciais_numero_de_patente_invencao )
		    		set_value ( RB_MODELO_UTILIDADE, iniciais_numero_de_modelo_utilidade )
		    		set_value ( RB_CERTIFICADO_ADICAO, iniciais_numero_de_certificado_adicao )
		    		set_enable ( RB_PATENTE_INVENCAO, iniciais_numero_de_patente_invencao )
		    		set_enable ( RB_MODELO_UTILIDADE, iniciais_numero_de_modelo_utilidade )
		    		set_enable ( RB_CERTIFICADO_ADICAO, iniciais_numero_de_certificado_adicao )
		    	'02':
		    		set_value ( RB_PATENTE_INVENCAO, true )
		    		set_value ( RB_MODELO_UTILIDADE, false )
		    		set_value ( RB_CERTIFICADO_ADICAO, false )
		    		set_enable ( RB_PATENTE_INVENCAO, true )
		    		set_enable ( RB_MODELO_UTILIDADE, false )
		    		set_enable ( RB_CERTIFICADO_ADICAO, false )
		    	'03':
		    		set_value ( RB_PATENTE_INVENCAO, false )
		    		set_value ( RB_MODELO_UTILIDADE, true )
		    		set_value ( RB_CERTIFICADO_ADICAO, false )
		    		set_enable ( RB_PATENTE_INVENCAO, false )
		    		set_enable ( RB_MODELO_UTILIDADE, true )
		    		set_enable ( RB_CERTIFICADO_ADICAO, false )
		    	'13':
		    		set_value ( RB_PATENTE_INVENCAO, true )
		    		set_value ( RB_MODELO_UTILIDADE, false )
		    		set_value ( RB_CERTIFICADO_ADICAO, false )
		    		set_enable ( RB_PATENTE_INVENCAO, true )
		    		set_enable ( RB_MODELO_UTILIDADE, false )
		    		set_enable ( RB_CERTIFICADO_ADICAO, false )
		    	otherwise
		    		set_value ( RB_PATENTE_INVENCAO, false )
		    		set_value ( RB_MODELO_UTILIDADE, false )
		    		set_value ( RB_CERTIFICADO_ADICAO, true )
		    		set_enable ( RB_PATENTE_INVENCAO, false )
		    		set_enable ( RB_MODELO_UTILIDADE, false )
		    		set_enable ( RB_CERTIFICADO_ADICAO, true )
		    end case			

		end TPN_NATUREZA
		
		gui_logical_group TPN_REQUER
		
			gui_element CB_REQUER_OUTROS
				type checkbox				
			end CB_REQUER_OUTROS
			
			gui_element MEM_REQUER_OUTROS
				type textbox
				
				case CB_REQUER_OUTROS
					true:
						enable true
						mandatory true
					false:
						enable false
						mandatory false
						value ''
				end case
				
			end MEM_REQUER_OUTROS
			
			gui_logical_group tipoRequerimento
				type radiogroup
					RB_REQUER_1
					RB_REQUER_2
					RB_REQUER_3
				end radiogroup				

			end tipoRequerimento
		
		end TPN_REQUER
		
	end TPN_PEDIDO

	/***********************************************************
	/* Validacoes da pagina Pedido
	/***********************************************************
	
	validation opcao_transferencia_gru_anexada_validacao
    	id 1019
    	level red
    	definition RB_REQUER_1 and ( get_count (SUBSET_GRU_249_ANEXADA_enable) = 0 )
    	id_string str_foi_apresentada_requerida( str_tranferencia_titular + anexar_gru_mensagem_generica_validacao(str_gru_249 + str_em_str(opcao_aba_gru_paga)))
    end opcao_transferencia_gru_anexada_validacao
	
	validation opcao_transferencia_validacao
    	id 1018
    	level red
    	definition RB_REQUER_1 and ( get_count (SUBSET_DOCUMENTO_TRANSFERENCIA_ANEXADO_enable) = 0 )
    	id_string str_foi_apresentada_requerida( str_tranferencia_titular + anexar_documento_mensagem_generica_validacao(str_tranferencia_titularidade + str_em_str(opcao_aba_documentos_gerais)))
    end opcao_transferencia_validacao
	
	validation opcao_cessao_transferencia_validacao
    	id 1022
    	level red
    	definition CB_REQUER_3 and ( get_count (SUBSET_DOCUMENTO_CESSAO_ANEXADO_enable) = 0 ) and ( get_count (SUBSET_DOCUMENTO_TRANSFERENCIA_ANEXADO_enable) = 0 )
    	id_string str_foi_apresentada_requerida( str_documento_cessao_transferencia + anexar_documento_mensagem_generica_validacao(str_cessao_transferencia + str_em_str(opcao_aba_documentos_gerais)))
    end opcao_cessao_transferencia_validacao
	
	validation opcao_procuracao_validacao
    	id 1017
    	level red
    	definition CB_REQUER_13 and ( get_count (SUBSET_DOCUMENTO_PROCURACAO_ANEXADA_enable) = 0 )
    	id_string str_foi_apresentada_requerida( str_procuracao + anexar_documento_mensagem_generica_validacao(str_procuracao + str_em_str(opcao_aba_documentos_gerais)))
    end opcao_procuracao_validacao
	
	validation opcao_alteracao_gru_anexada_validacao
    	id 1018
    	level red
    	definition ( RB_REQUER_2 or RB_REQUER_3 ) and ( get_count (SUBSET_GRU_248_ANEXADA_enable) = 0 )
    	id_string str_foi_apresentada_requerida( obter_tipo_alteracao + anexar_gru_mensagem_generica_validacao(str_gru_248 + str_em_str(opcao_aba_gru_paga)))
    end opcao_alteracao_gru_anexada_validacao
	
    validation opcao_alteracao_validacao
    	id 1021
    	level red
    	definition ( RB_REQUER_2 or RB_REQUER_3 or CB_REQUER_1 )  and ( get_count (SUBSET_DOCUMENTO_ALTERACAO_ANEXADO_enable) = 0 )
    	id_string str_foi_apresentada_requerida( obter_tipo_alteracao + anexar_documento_mensagem_generica_validacao(str_alteracao_nome_sede + str_em_str(opcao_aba_documentos_gerais)))
    end opcao_alteracao_validacao
	
	validation tipo_peticao_apresenta_validacao
    	id 1016
    	level red
    	definition ( not CB_REQUER_1 ) and ( not CB_REQUER_2 ) and ( not CB_REQUER_3 ) and ( not CB_REQUER_13 ) and ( not CB_REQUER_OUTROS )
    	id_string tipo_peticao_apresenta_mensagem_validacao
    end tipo_peticao_apresenta_validacao

	validation tipo_peticao_requer_validacao
    	id 1020
    	level red
    	definition ( not RB_REQUER_1 ) and ( not RB_REQUER_2 ) and ( not RB_REQUER_3 )
    	id_string tipo_peticao_requer_mensagem_validacao
    end tipo_peticao_requer_validacao
	
	validation descricao_outros_documentos_validacao
    	id 1015
    	level red
    	definition CB_REQUER_OUTROS and MEM_REQUER_OUTROS is empty
    	id_string descricao_outros_documentos_mensagem_validacao
    end descricao_outros_documentos_validacao
	
		validation titulo_em_branco_validacao
		id 1001
		level red
		definition ED_TITULO_INVENCAO is empty
		id_string titulo_em_branco_mensagem_validacao
	end titulo_em_branco_validacao
	
	validation titulo_tamanho_validacao
		id 1011
		level red
		definition length ( ED_TITULO_INVENCAO ) > 9000
		id_string titulo_tamanho_mensagem_validacao
	end titulo_tamanho_validacao

    validation numero_pedido_dv_validacao_PI_MU_CA
    	id 1014
    	level red
    	definition (    					
    					( get_value (CB_NUM_CERT_ADICAO)<>'01') and ED_NUM_CERT_ADICAO is not empty and
    					( not checkregnumberno (REG_NUM_CERT_ADICAO,'rnmBRINPIPI'))
    				)
    	id_string numero_pedido_mensagem_validacao
    end numero_pedido_dv_validacao_PI_MU_CA
  
    validation numero_certificado_adicao_dv_validacao_BR
    	id 1013
    	level red
    	definition	( get_value (CB_NUM_CERT_ADICAO)='01') and ( ED_NUM_CERT_ADICAO is not empty) and
					(
						( not checkregnumberno (REG_NUM_CERT_ADICAO,'rnmBRINPIBR'))
						or
						(
							( checkregnumberno (REG_NUM_CERT_ADICAO,'rnmBRINPIBR')) and
							( not ED_NUM_CERT_ADICAO in mask '1[0-3]{1}[0-9]{11}') and
							( not ED_NUM_CERT_ADICAO in mask '2[0-2]{1}[0-9]{11}')
						)
					)
    	id_string numero_pedido_mensagem_validacao
    end numero_certificado_adicao_dv_validacao_BR

	validation ccbb_tipo_de_pedido_em_branco_validacao
		id 1009
		level red
		definition CB_NUM_CERT_ADICAO is empty
		id_string ccbb_tipo_de_pedido_em_branco_mensagem_validacao
	end ccbb_tipo_de_pedido_em_branco_validacao
	
	validation numero_do_pedido_em_branco_validacao
		id 1012
		level red
		definition  ( CB_NUM_CERT_ADICAO is not empty ) and ( ED_NUM_CERT_ADICAO is empty )
		id_string ccbb_tipo_de_pedido_em_branco_mensagem_validacao
	end numero_do_pedido_em_branco_validacao
	
end PG_PEDIDO

